{
  "$schema": "http://json-schema.org/schema#",
  "properties": {
    "affinity": {
      "type": "object"
    },
    "autoscaling": {
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "maxReplicas": {
          "type": "integer"
        },
        "minReplicas": {
          "type": "integer"
        },
        "targetCPUUtilizationPercentage": {
          "type": "integer"
        }
      },
      "type": "object"
    },
    "awsSecretName": {
      "type": "string"
    },
    "config": {
      "properties": {
        "dryRun": {
          "type": "boolean"
        },
        "logFormat": {
          "type": "string"
        },
        "logLevel": {
          "type": "string"
        },
        "source": {
          "properties": {
            "filters": {
              "items": {
                "properties": {
                  "jmespath": {
                    "type": "string"
                  }
                },
                "type": "object"
              },
              "type": "array"
            }
          },
          "type": "object"
        },
        "target": {
          "properties": {
            "aws": {
              "properties": {
                "accountId": {
                  "type": "integer"
                },
                "region": {
                  "type": "string"
                }
              },
              "type": "object"
            }
          },
          "type": "object"
        }
      },
      "type": "object"
    },
    "dev": {
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "webhookURL": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "fullnameOverride": {
      "type": "string"
    },
    "image": {
      "properties": {
        "pullPolicy": {
          "type": "string"
        },
        "repository": {
          "type": "string"
        },
        "tag": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "imagePullSecrets": {
      "type": "array"
    },
    "nameOverride": {
      "type": "string"
    },
    "nodeSelector": {
      "type": "object"
    },
    "patch": {
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "image": {
          "properties": {
            "pullPolicy": {
              "type": "string"
            },
            "repository": {
              "type": "string"
            },
            "tag": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "nodeSelector": {
          "type": "object"
        },
        "podAnnotations": {
          "type": "object"
        },
        "priorityClassName": {
          "type": "string"
        },
        "resources": {
          "type": "object"
        }
      },
      "type": "object"
    },
    "certManager": {
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "issuerRef": {
          "properties": {
            "name": {
              "type": "string"
            },
            "kind": {
              "type": "string"
            }
          },
          "type": "object"
        }
      },
      "type": "object"
    },
    "podAnnotations": {
      "type": "object"
    },
    "podSecurityContext": {
      "type": "object"
    },
    "podSecurityPolicy": {
      "properties": {
        "enabled": {
          "type": "boolean"
        }
      },
      "type": "object"
    },
    "rbac": {
      "properties": {
        "create": {
          "type": "boolean"
        }
      },
      "type": "object"
    },
    "replicaCount": {
      "type": "integer"
    },
    "resources": {
      "type": "object"
    },
    "securityContext": {
      "type": "object"
    },
    "service": {
      "properties": {
        "port": {
          "type": "integer"
        },
        "type": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "serviceAccount": {
      "properties": {
        "annotations": {
          "type": "object"
        },
        "create": {
          "type": "boolean"
        },
        "name": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "tolerations": {
      "type": "array"
    },
    "webhook": {
      "properties": {
        "failurePolicy": {
          "type": "string"
        }
      },
      "type": "object"
    }
  },
  "type": "object"
}
